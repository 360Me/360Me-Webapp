  "devDependencies": {
    "babel-core": "^4.7.8",
    "babel-loader": "^4.1.0",
    "coveralls": "^2.11.2",
    "express": "^4.12.2",
    "istanbul": "^0.3.7",
    "istanbul-instrumenter-loader": "^0.1.2",
    "jest-cli": "^0.4.0",
    "react-hot-loader": "^1.2.3",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0",
